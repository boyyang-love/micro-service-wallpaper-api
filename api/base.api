syntax = "v1"

type Base {
    Code int `json:"code"`
    Msg string `json:"msg"`
}

type BasePage {
    Page int `form:"page"`
    Limit int `form:"limit"`
}

type BaseRecord {
    Page int `json:"page"`
    Limit int `json:"limit"`
    Total int64 `json:"total"`
}

type BaseTime {
    Created int64 `json:"created"`
    Updated int64 `json:"updated"`
}